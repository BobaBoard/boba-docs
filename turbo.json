{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "start": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "serve": {
      "dependsOn": ["^build"],
      "outputs": []
    }
  }
}
